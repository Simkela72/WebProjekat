<!DOCTYPE html>
<html>
<head>
    <title>Lista pretplatnika</title>
</head>
<body>
    <h2>Lista korisnika prijavljenih na trening</h2>
    <table border="1">
        <tr>
            <th>Korisnicko ime</th>
        </tr>
        @foreach (var subscriber in ViewBag.Pretplatnici)
        {
            <tr>
                <td>@subscriber</td>
            </tr>
        }
    </table>
    <form action="@Url.Action("LogOut")" method="post">
        <button type="submit">Log out</button>
    </form>
</body>
</html>